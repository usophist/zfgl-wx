<view class="page container"><viwe class="top-search" vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><view style="flex:1;"><search-item vue-id="{{('2')+','+('1')}}" placeholder="请输入租客名" data-event-opts="{{[['^getKeyWord',[['search']]]]}}" bind:getKeyWord="__e" bind:__l="__l"></search-item></view><label class="iconfont _span" style="margin-right:35rpx;color:#333;"></label></viwe><viwe class="bill-type" vue-id="3" bind:__l="__l" vue-slots="{{['default']}}"><view data-event-opts="{{[['tap',[['paramsChange',[['o',['type',0]]]]]]]}}" class="{{[params.type==0?'.btn-type-active':'btn-type']}}" bindtap="__e"><text>全部</text></view><viw class="type-split" vue-id="{{('4')+','+('3')}}" bind:__l="__l"></viw><view data-event-opts="{{[['tap',[['paramsChange',[['o',['type',-1]]]]]]]}}" class="{{[params.type==-1?'.btn-type-active':'btn-type']}}" bindtap="__e"><text>未收</text></view><viw class="type-split" vue-id="{{('5')+','+('3')}}" bind:__l="__l"></viw><view data-event-opts="{{[['tap',[['paramsChange',[['o',['type',1]]]]]]]}}" class="{{[params.type==1?'.btn-type-active':'btn-type']}}" bindtap="__e"><text>已收</text></view></viwe><block wx:for="{{bills}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['showPage',['../../pages/bill/billDetails']]]]]}}" class="bill-view" bindtap="__e"><view class="bill-left-view"><text class="bill-left-text1">{{item.name}}</text><text class="bill-left-text2">{{item.state}}</text></view><text class="bill-right-text">{{item.money}}</text></view></block></view>